#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x83f730 .scope module, "test_clock" "test_clock" 2 3;
 .timescale 0 0;
v0x83f820_0 .var "clk", 0 0;
v0x881f00_0 .var "clk2", 0 0;
    .scope S_0x83f730;
T_0 ;
    %set/v v0x83f820_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x83f730;
T_1 ;
    %set/v v0x881f00_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x83f730;
T_2 ;
    %delay 10, 0;
    %load/v 8, v0x83f820_0, 1;
    %inv 8, 1;
    %set/v v0x83f820_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x83f730;
T_3 ;
    %delay 20, 0;
    %load/v 8, v0x881f00_0, 1;
    %inv 8, 1;
    %set/v v0x881f00_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x83f730;
T_4 ;
    %vpi_call 2 15 "$monitor", "%d %b %b", $time, v0x83f820_0, v0x881f00_0;
    %delay 100, 0;
    %vpi_call 2 16 "$finish";
    %end;
    .thread T_4;
    .scope S_0x83f730;
T_5 ;
    %vpi_call 2 20 "$dumpfile", "test_clock.vcd";
    %vpi_call 2 21 "$dumpvars", 1'sb0, S_0x83f730;
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "test_clock.v";
